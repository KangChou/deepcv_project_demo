{
  "manifest_version": 2,

  "name": "FreeVPN Free VPN Proxy",
  "short_name": "Free VPN",
  "description": "Unblock websites and stay secure with FreeVPN SSL VPN proxy.",
  "version": "21.9.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "48": "images/off.png",
      "64": "images/off.png",
      "128": "images/off.png"
    },
    "default_popup": "popup.html",
    "default_title": "FreeVPN"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

  "permissions": [
    "proxy",
    "storage",
    "alarms",
    "notifications"
  ]
}